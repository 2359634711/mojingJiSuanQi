/* components/navBottom/navBottom.wxss */

.navBottom {
  width: 100%;
  height: 110rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  z-index: 2;
}

.item {
  font-size: 22rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icon {
  width: 44rpx;
  height: 44rpx;
  margin-bottom: 14rpx;
}

.itemActive {
  color: var(--themeC);
}
